{
  "installation": {
    "version": "21.3.7"
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "projects": {
    "front": "apps/front",
    "go": "apps/go"
  },
  "targetDefaults": {
    "@angular/build:application": {
      "cache": true,
      "dependsOn": ["^build"],
      "inputs": ["default", "^default"]
    }
  },
  "generators": {
    "@nx/angular:application": {
      "e2eTestRunner": "none",
      "linter": "eslint",
      "style": "scss",
      "unitTestRunner": "jest"
    },
    "@nx/angular:component": {
      "style": "css"
    }
  }
}
